{
  "expo": {
    "name": "iLeaf-mobile",
    "slug": "iLeaf-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ileaf-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/ileaf-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ileaf.store",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ileaf-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.iLeaf"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1141a67b-78ef-4c8b-9dd7-303da17475c1"
      }
    },
    "owner": "donniemix"
  }
}
